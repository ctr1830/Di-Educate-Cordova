<?xml version='1.0' encoding='utf-8'?>
<widget id="eus.ehu.intel.tta.di_educate" version="0.0.1"
	xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Di Educate</name>
	<description>
		The best application for kids with speaking difficulties
	</description>
	<author href="http://www.eclipse.org/thym" email="thym-dev@eclipse.org">
		Cristian
		Llaguno Iglesias
	</author>

	<content src="main.html" />
	<!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<plugin name="cordova-plugin-screen-orientation" spec="1" />
	<plugin name="cordova-plugin-media-capture" spec="1" />
	<plugin name="cordova-plugin-file" spec="1" />
	<plugin name="cordova-plugin-file-transfer" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	<preference name="Orientation" value="portrait" />

	
<engine name="android" spec="6.1.0-nightly.2016.10.21.e628026c"></engine></widget>
